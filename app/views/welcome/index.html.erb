<div>
  <br />
  <p><%= link_to("All Aliens", aliens_path) %></p>
  
  <p>Alien <input type="text" id="show_id" placeholder="id" size="4" maxlength="4">
  <a onclick="this.href='aliens/' + document.getElementById('show_id').value">show</a></p>

  <p><%= link_to "Create New Alien", new_alien_path %></p>

  <p><%= link_to "Update Alien #1", edit_alien_path(1) %></p>

  <p>Delete Alien <input type="text" id="delete_id" placeholder="id" size="4" maxlength="4">
  <a data-method='delete' data-confirm="Are you sure you want to delete this alien?" onclick="this.href='aliens/' + document.getElementById('delete_id').value">delete</a></p>
  <hr>
  
  <p><%= link_to "Get all Powers for Alien #35", alien_powers_path(35) %></p>

  <p><%= link_to "Show Power #51 for Alien #35", alien_power_path(35, 51) %></p>

  <p><%= link_to "Create New Power for Alien #35", new_alien_power_path(35) %></p>

  <p><%= link_to "Update Power #51 for Alien #35", edit_alien_power_path(35, 17) %></p>

  <p><%= link_to "Delete Power #72 for Alien #35", alien_power_path(35, 72), method: :delete, data: { confirm: 'Are you sure you want to delete this power?' } %></p>
</div>